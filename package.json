{
	"name": "frontend",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"packageManager": "pnpm@7.18.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync && houdini generate",
		"check": "pnpm run sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "pnpm run sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@poppanator/sveltekit-svg": "^2.0.2",
		"@sveltejs/adapter-auto": "^1.0.1",
		"@sveltejs/kit": "^1.15.2",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql": "^15.8.0",
		"houdini": "^0.19.4",
		"houdini-svelte": "^0.19.4",
		"postcss": "^8.4.31",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.5"
	},
	"dependencies": {
		"@fontsource/noto-sans": "^4.5.11",
		"@fontsource/noto-sans-jp": "^4.5.12",
		"@fontsource/rubik": "^4.5.13"
	}
}
